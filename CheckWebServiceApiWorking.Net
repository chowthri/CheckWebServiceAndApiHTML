using System;
using System.Net.Http;
using System.Threading.Tasks;
using System.Collections.Generic;

class Program
{
    static async Task Main(string[] args)
    {
        // Define your list of IP addresses
        List<string> ipAddresses = new List<string>()
        {
            "192.168.1.1",
            "192.168.1.2",
            "192.168.1.3"
        };

        // Define the endpoint (assuming the web service is hosted at /service.asmx?WSDL)
        string endpoint = "/service.asmx?WSDL";

        // Loop through each IP address and check accessibility
        using (HttpClient client = new HttpClient())
        {
            foreach (string ip in ipAddresses)
            {
                string url = $"http://{ip}{endpoint}";

                try
                {
                    // Send a GET request to the web service
                    HttpResponseMessage response = await client.GetAsync(url);

                    // Check if the service is accessible (HTTP status 200 OK)
                    if (response.IsSuccessStatusCode)
                    {
                        Console.WriteLine($"Web service is accessible at {url} (Status: {response.StatusCode})");
                    }
                    else
                    {
                        Console.WriteLine($"Web service at {url} returned status code: {response.StatusCode}");
                    }
                }
                catch (HttpRequestException ex)
                {
                    // Handle any network-related or service access errors
                    Console.WriteLine($"Error accessing web service at {url}: {ex.Message}");
                }
            }
        }
    }
}
